{
  "hidden_layers": [
    {"class": "FlattenAxis", "axis": 3},
    {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
    {"class": "Conv2DTranspose", "filters": 64, "kernel_size": 3, "strides": 1, "activation": null},
    {"class": "Cropping2D", "cropping": 4},
    {"alpha": 0.2, "class": "LeakyReLU"},
    {"class": "SkipConnection", "name": "a"},

    {"n": 8, "repeat": [
        {"class": "SkipConnection", "name": "b"},
        {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
        {"class": "Conv2DTranspose", "filters": 64, "kernel_size": 3, "strides": 1, "activation": null},
        {"class": "Cropping2D", "cropping": 4},
        {"alpha": 0.2, "class": "LeakyReLU"},
        {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
        {"class": "Conv2DTranspose", "filters": 64, "kernel_size": 3, "strides": 1, "activation": null},
        {"class": "Cropping2D", "cropping": 4},
        {"class": "SkipConnection", "name": "b"}
      ]
    },

    {"class": "SkipConnection", "name": "a"},
    {"class": "ExpandDims", "axis": 3},
    {"class": "SpatioTemporalExpansion", "spatial_mult": 1, "temporal_mult": 24},
    {"class": "GaussianNoiseAxis", "axis": 3, "mean": 1, "stddev": 0.1},

    {"n": 8, "repeat": [
        {"class": "SkipConnection", "name": "c"},
        {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
        {"class": "Conv3D", "filters": 64, "kernel_size": 3, "strides": 1},
        {"class": "Cropping3D", "cropping": 2},
        {"alpha": 0.2, "class": "LeakyReLU"},
        {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
        {"class": "Conv3D", "filters": 64, "kernel_size": 3, "strides": 1},
        {"class": "Cropping3D", "cropping": 2},
        {"class": "SkipConnection", "name": "c"}
      ]
    },

    {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
    {"class": "Conv3D", "filters": 64, "kernel_size": 3, "strides": 1, "activation": null},
    {"class": "Cropping3D", "cropping": 2},
    {"alpha": 0.2, "class": "LeakyReLU"},

    {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
    {"class": "Conv3D", "filters": 64, "kernel_size": 3, "strides": 1, "activation": null},
    {"class": "Cropping3D", "cropping": 2},
    {"class": "SpatioTemporalExpansion", "spatial_mult": 2, "temporal_mult": 1},
    {"alpha": 0.2, "class": "LeakyReLU"},

    {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
    {"class": "Conv3D", "filters": 200, "kernel_size": 3, "strides": 1, "activation": null},
    {"class": "Cropping3D", "cropping": 2},
    {"class": "SpatioTemporalExpansion", "spatial_mult": 2, "temporal_mult": 1},
    {"alpha": 0.2, "class": "LeakyReLU"},

    {"class": "FlexiblePadding", "paddings": [[0,0], [3,3], [3,3], [3,3], [0,0]], "mode": "REFLECT"},
    {"class": "Conv3D", "filters": 4, "kernel_size": 3, "strides": 1, "activation": null},
    {"class": "Cropping3D", "cropping": 2}
  ]
}
